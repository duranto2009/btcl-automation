<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>
    <form-beans>
        <form-bean name="asn" type="login.LoginForm"/>
    </form-beans>
    <action-mappings>
        <action path="asn/**" type="lli.asn.ASNAction" name="asn">
            <forward name="asn" path="/lli/asn/asn.jsp"></forward>
        </action>
    </action-mappings>
</struts-config>